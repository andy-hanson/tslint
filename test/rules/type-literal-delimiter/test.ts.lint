type T = {
    x: number,
             ~ [PREFER_SEMI]
    y: string
            ~ [MISSING]
};

type T = {
    x: number
            ~ [MISSING]
    y: string,
             ~ [PREFER_SEMI]
};

type T = {
    x: number;
    y: string;
};

type T = { x: number, y: string };

// Works even when there's extra whitespace
type T = { x: number , y: number };
type T = { x: number ; y: number , };
                     ~ [PREFER_COMMA]
                                 ~ [EXTRA % (',')]
type T = { x: number , y: number ; };
                                 ~ [EXTRA % (';')]

[MISSING]: Expected multi-line type literal to have a trailing ';'.
[PREFER_SEMI]: Expected multi-line type literal to use ';' instead of ','.
[PREFER_COMMA]: Expected single-line type literal to use ',' instead of ';'.
[EXTRA]: Did not expect single-line type literal to have a trailing '%s'.
