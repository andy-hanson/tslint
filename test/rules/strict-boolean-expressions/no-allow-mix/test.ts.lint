declare function get<T>(): T;

if (get<string | number>()) {}
    ~~~~~~~~~~~~~~~~~~~~~~ [If statement condition may be falsy if it is 0. Expected a boolean, undefined-union, or string.]

if (get<string | boolean>()) {}
    ~~~~~~~~~~~~~~~~~~~~~~~ [mix]

if (get<string | undefined>()) {}
    ~~~~~~~~~~~~~~~~~~~~~~~~~ [mix]

if (get<boolean>()) {}
if (get<string>()) {}
if (get<RegExp | undefined>()) {}

if (get<"" | "foo">()) {}

// Mix of truthy values OK
if (get<"foo" | "bar" | undefined>()) {}

// Mix of falsy values not OK
if (get<"" | boolean>()) {}
    ~~~~~~~~~~~~~~~~~~~ [mix]

[mix]: If statement condition mixes multiple kinds of falsy values. Expected a boolean, undefined-union, or string.
